@model EntityLayer.Concrete.Admin

@{
    ViewData["Title"] = "AddAdmin";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<div class="col-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Yeni Kullanıcı Giriş Sayfası</h4>
            <form method="post" class="forms-sample">
                <div class="form-group">
                    <label>Username</label>
                    <input type="text" class="form-control" asp-for="Username" placeholder="başlık">
                </div>
                <div class="form-group">
                    <label>Password</label>
                    <input type="password" class="form-control" asp-for="Password" colls="30" rows="4"></input>
                </div>
                <button type="submit" class="btn btn-outline-info">Kaydet</button>
            </form>
        </div>
    </div>
</div>


